check process paas-ta-portal-storage-api
  with pidfile /var/vcap/sys/run/paas-ta-portal-storage-api/paas-ta-portal-storage-api.pid
  start program "/var/vcap/jobs/paas-ta-portal-storage-api/bin/monit_debugger service_ctl '/var/vcap/jobs/paas-ta-portal-storage-api/bin/service_ctl start'"
  stop program "/var/vcap/jobs/paas-ta-portal-storage-api/bin/monit_debugger service_ctl '/var/vcap/jobs/paas-ta-portal-storage-api/bin/service_ctl stop'"
  group vcap
